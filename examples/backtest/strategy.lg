backtest VariableHoldUntil4 {
    InitialCapital  10000
    StartTime       "2010-01-01"
    EndTime         "2010-12-31"
    Indicators       [sma(20) as sma20, sma(50) as sma50, sma(200) as sma200]

    Strategy {
        if (sma20 < sma50) {
            return Buy("SPY", 100)
        }

        return Buy("SPY", 200)
    }
}